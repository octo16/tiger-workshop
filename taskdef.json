{
  "executionRoleArn": "arn:aws:iam::621244614945:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "workshop-dogs",
      "image": "621244614945.dkr.ecr.ap-northeast-2.amazonaws.com/workshop-dogs:latest",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "protocol": "tcp",
          "containerPort": 80
        }
      ],
      "linuxParameters": {
	      "initProcessEnabled": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "workshop-dogs"
}
